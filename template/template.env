# 麦麦主程序配置
HOST=127.0.0.1
PORT=8000

# 数据库配置（供 maim_db 使用）
DATABASE_URL=sqlite:///data/MaiBot.db
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=maimbot
DATABASE_USER=postgres
DATABASE_PASSWORD=

# 日志与调试级别
MAIMBOT_LOG_LEVEL=INFO
MAIMBOT_CONSOLE_LOG_LEVEL=INFO
MAIMBOT_FILE_LOG_LEVEL=DEBUG

# maim_message 服务器
MAIM_MESSAGE_HOST=127.0.0.1
MAIM_MESSAGE_PORT=8090
MAIM_MESSAGE_MODE=ws
MAIM_MESSAGE_USE_WSS=false

# WebUI 独立服务器配置
WEBUI_ENABLED=true
WEBUI_MODE=production   # 模式: development(开发) 或 production(生产)
WEBUI_HOST=0.0.0.0      # WebUI 服务器监听地址
WEBUI_PORT=8001         # WebUI 服务器端口